{
  "metrics": [
    {
      "name": "CPU Usage",
      "type": "time_series",
      "description": "CPU usage is the percentage of time that the CPU is being used to complete its tasks.",
      "internal": true,
      "show": true,
      "unit": "%",
      "series": [
        {
          "name": "Cpu",
          "field": "cpu_usage",
          "show": true,
          "config": {
            "lineWidth": 1,
            "barWidth": 20,
            "area": {
              "show": true,
              "opacity": 50
            },
            "type": "line",
            "color": "#0991B3"
          }
        }
      ],
      "config": {
        "histogram": {
          "bucket": {
            "size": 5,
            "offset": 0
          },
          "min": 0,
          "max": null
        },
        "stack": {
          "show": false,
          "strategy": "samesign"
        },
        "line": {
          "marker": {
            "show": false,
            "shape": "react"
          }
        },
        "tooltip": {
          "show": false,
          "position": "bottom"
        },
        "legend": {
          "show": true,
          "orient": "horizontal"
        },
        "axis": {
          "showX": true,
          "showY": true,
          "showGridLines": true
        }
      }
    },
